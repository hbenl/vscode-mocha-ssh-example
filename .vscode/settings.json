{
  "files.exclude": {
    "node_modules": true
  },
  "editor.insertSpaces": false,
  "editor.tabSize": 4,
  "[json]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "mochaExplorer.launcherScript": "node_modules/mocha-explorer-launcher-scripts/ssh",
  "mochaExplorer.env": {
    "SSH_HOST": "host",
    "SSH_USER": "user",
    "SSH_WORKSPACE_PATH": "/vscode-mocha-ssh",
    "TEST_ENVVAR": "TEST_VALUE"
  },
  "mochaExplorer.require": [ "required.js" ],
  "mochaExplorer.debuggerConfig": "Debug via ssh"
}
